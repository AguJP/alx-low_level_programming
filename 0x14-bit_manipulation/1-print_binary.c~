#include "main.h"

/**
 * print_binary - Function to print the binary representation of a no.
 * @n: integer no. to be converted to binary
 * Return: binary no.
 */

void print_binary(unsigned long int n)
{
	int i, count = 0;
	unsigned long int current;

	for (i = 63; i >= 0; i--)
	{
		current = n >> i;

		if (current & 1)
		{
